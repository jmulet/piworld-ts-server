{
    "name": "piworld-ts-client",
    "version": "1.0.0",
    "description": "Server for piworld site based on nodejs + springjs",
    "main": "index.js",
    "scripts": {
        "static:prod": "webpack --progress --config ./webpack3/static-prod.config.js",
        "static:dev": "webpack --progress --watch --config ./webpack3/static-dev.config.js",
        "clean": "rm -fr ./compiled && rm -fr ./dist/public",
        "angularcompile": "npm run-script clean && ./node_modules/.bin/ngc -p ./tsconfig-aot.json",
        "serve": "npm run-script clean &&   webpack --watch --progress --config ./webpack3/dev.config.js",
        "compile": "npm run-script angularcompile && cp -r ./node_modules/primeng ./compiled/node_modules && npm run-script static:prod && webpack --progress  --config  ./webpack3/prod.config.js",
        "build": "npm run-script compile"
    },
    "keywords": [
        "client",
        "angularjs",
        "mvc",
        "piworld"
    ],
    "author": "Josep Mulet",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jmulet/piworld-ts-server/issues"
    },
    "homepage": "https://github.com/jmulet/piworld-ts-server#readme",
    "dependencies": {
        "@angular/animations": "^5.2.7",
        "@angular/common": "^5.2.7",
        "@angular/compiler": "^5.2.7",
        "@angular/core": "^5.2.7",
        "@angular/forms": "^5.2.7",
        "@angular/platform-browser": "^5.2.7",
        "@angular/platform-browser-dynamic": "^5.2.7",
        "@angular/router": "^5.2.7",
        "angular": "^1.6.9",
        "angular-multiple-select": "^1.1.3",
        "angular-sanitize": "^1.6.9",
        "angular-schema-form": "^0.8.13",
        "angular-ui-bootstrap": "^2.5.6",
        "bootstrap": "^4.0.0",
        "classlist.js": "^1.1.20150312",
        "component-emitter": "^1.2.1",
        "core-js": "^2.5.3",
        "crypto-js": "^3.1.9-1",
        "ejson": "^2.1.2",
        "fastclick": "^1.0.6",
        "font-awesome": "^4.4.0",
        "intl": "^1.2.5",
        "ionicons": "^2.0.1",
        "jquery": "^3.3.1",
        "jquery-slimscroll": "^1.3.8",
        "katex": "^0.9.0",
        "ng-toggle": "^1.2.0",
