<!doctype html>
<html lang="<%=lang%>">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="piWorld site">
  <meta name="author" content="Josep Mulet">
  <link rel="icon" href="/favicon.ico">
  <title>piWorld 404</title> 
  <link href="<%-prefixUrl('/login.'+config.version+'.bundle.css')%>" rel="stylesheet">
</head>

<body>
  <div class="container">
    <div class="form-signin">
      <center>
        <img src="<%-prefixUrl('/assets/img/logo2.png')%>" style="width:100px" />
      </center>
      <div class="alert alert-warning" style="text-align:center">
        <h1>404</h1>
        <h3>
          <%=translations["404"]%>
        </h3>
      </div>

    </div>
  </div>

  <footer class="main-footer">
    <center>
      <a class="btn btn-lg btn-primary" href="<%-prefixUrl('/desktop.htm')%>">
        <i class="glyphicon glyphicon-home"></i>
        <%=translations["HOME"]%>        
      </a>
    </center>
  </footer>

  <script>
    if(typeof(parent) !== 'undefined') {
        document.getElementsByClassName("main-footer")[0].style.display = "none";
    }
  </script>
</body>

</html>
</html>